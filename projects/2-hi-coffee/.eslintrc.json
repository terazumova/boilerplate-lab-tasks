{
    "parserOptions": {
      "browser": true,
      "ecmaVersion": 2021
    },
    "env": {
      "es6": true
    },
    "extends": [
      "plugin:coffee/airbnb-base"
    ],
    "parser": "eslint-plugin-coffee",
    "plugins": ["coffee"],
    "rules": {
        "no-plusplus": "off",
        "no-unused-vars": "off",
        "coffee/no-unused-vars": "off",
        "@typescript-eslint/no-unused-vars": "off"
    }
}
